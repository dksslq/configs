{
    "dns": null,
    "inbounds": [
        {
            "listen": "127.0.0.1",
            "port": 10808,
            "protocol": "socks",
            "settings": {
                "accounts": [
                    {
                        "pass": "1234",
                        "user": "foo"
                    }
                ],
                "auth": "password",
                "clients": null,
                "ip": null,
                "udp": true
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls"
                ],
                "enabled": true
            },
            "streamSettings": null,
            "tag": "example-tag-socks-inbound"
        },
        {
            "listen": "127.0.0.1",
            "port": 10809,
            "protocol": "http",
            "settings": {
                "accounts": [
                    {
                        "pass": "1234",
                        "user": "bar"
                    }
                ],
                "auth": "password",
                "clients": null,
                "ip": null,
                "udp": true
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls"
                ],
                "enabled": true
            },
            "streamSettings": null,
            "tag": "example-tag-http-inbound"
        }
    ],
    "log": {
        "access": "",
        "error": "",
        "loglevel": "warning"
    },
    "outbounds": [
        {
            "mux": {
                "enabled": false
            },
            "protocol": "vmess",
            "settings": {
                "response": null,
                "servers": null,
                "vnext": [
                    {
                        "address": "OUT_ADDR",
                        "port": 10086,
                        "users": [
                            {
                                "alterId": 64,
                                "email": "foo@mail",
                                "id": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
                                "security": "chacha20-poly1305"
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "httpSettings": null,
                "kcpSettings": null,
                "network": "ws",
                "quicSettings": null,
                "security": "tls",
                "tcpSettings": null,
                "tlsSettings": {
                    "allowInsecure": false,
                    "alpn": [
                        "http/1.1"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "cert.pem",
                            "usage": "verify"
                        }
                    ],
                    "disableSystemRoot": true,
                    "serverName": "HOST_NAME"
                },
                "wsSettings": {
                    "connectionReuse": true,
                    "headers": {
                        "Host": "HOST_NAME"
                    },
                    "path": "/path/foo"
                }
            },
            "tag": "example-tag-vmess-outbound"
        },
        {
            "mux": null,
            "protocol": "freedom",
            "settings": {
                "response": null,
                "servers": null,
                "vnext": null
            },
            "streamSettings": null,
            "tag": "direct"
        },
        {
            "mux": null,
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                },
                "servers": null,
                "vnext": null
            },
            "streamSettings": null,
            "tag": "block"
        }
    ],
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": []
    }
}
