{
  "log": {
    "access": "",
    "error": "",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "tag": "example-tag-socks-inbound",
      "port": 10808,
      "listen": "127.0.0.1",
      "protocol": "socks",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      },
      "settings": {
        "auth": "password",
        "accounts": [
          {
            "user": "foo",
            "pass": "1234"
          }
        ],
        "udp": true,
        "ip": null,
        "clients": null
      },
      "streamSettings": null
    },
    {
      "tag": "example-tag-http-inbound",
      "port": 10809,
      "listen": "127.0.0.1",
      "protocol": "http",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      },
      "settings": {
        "auth": "password",
        "accounts": [
          {
            "user": "bar",
            "pass": "1234"
          }
        ],
        "udp": true,
        "ip": null,
        "clients": null
      },
      "streamSettings": null
    }
  ],
  "outbounds": [
    {
      "tag": "example-tag-vmess-outbound",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "OUT_ADDR",
            "port": 443,
            "users": [
              {
                "id": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
                "alterId": 64,
                "email": "foo@mail",
                "security": "chacha20-poly1305"
              }
            ]
          }
        ],
        "servers": null,
        "response": null
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
          "serverName": "HOST_NAME",
          "allowInsecure": false,
          "alpn": ["http/1.1"],
          "certificates": [
            {
              "usage": "verify",
              "certificateFile": "cert.pem"
            }
          ],
          "disableSystemRoot": true
        },
        "tcpSettings": null,
        "kcpSettings": null,
        "wsSettings": {
          "connectionReuse": true,
          "path": "/path/foo",
          "headers": {
            "Host": "HOST_NAME"
          }
        },
        "httpSettings": null,
        "quicSettings": null
      },
      "mux": {
        "enabled": true
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {
        "vnext": null,
        "servers": null,
        "response": null
      },
      "streamSettings": null,
      "mux": null
    },
    {
      "tag": "block",
      "protocol": "blackhole",
      "settings": {
        "vnext": null,
        "servers": null,
        "response": {
          "type": "http"
        }
      },
      "streamSettings": null,
      "mux": null
    }
  ],
  "dns": null,
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": []
  }
}
